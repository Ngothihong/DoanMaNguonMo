                                <li class="divider"></li>
                                <li><a href="#"><i class="fa fa-power-off"></i> Đăng Xuất</a></li>
                            </ul>
                        </li>
                        <li class="divider-vertical"></li>
                    </ul>
                </div>
            </nav>

            @RenderBody();


        </div>
    </div>
    <!-- /#wrapper -->
    <script>
        function themGV() {
            var lop = document.getElementById('txtName').value;
            var gv = document.getElementById('txtgv').value;
            $.ajax({
                url: "/Phanbo_giangday/themGV",
                type: 'Post',
                data: { lop: lop, gv: gv },
                success: function (result) {
                    alert(result);

                },
                error: function (xhr) {
                    console.log(xhr.responseText);
                    alert("Error has occurred..");
                }
            });
        }
    </script>
</body>
</html>
